import Foundation
extension DispatchQueue{
    private static var _onceTracker = [String]()
    
    public class func once(token: String, closure: ()->()){
        objc_sync_enter(self)
        
        defer {
            objc_sync_exit(self)
        }
        
        if _onceTracker.contains(token){
            return
        }
        
        _onceTracker.append(token)
        closure()
    }
}
